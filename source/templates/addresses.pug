include settings/variables
include components/mixins.pug
<!DOCTYPE html>
html(lang="es")
  head
    meta(charset="UTF-8")
    title Tienda FitoEspinosa
    link(rel="shortcut icon" href="http://fitoespinosa.com/wp-content/uploads/2018/03/favicon.png")
    link(rel="stylesheet", href="dist/css/app.css")
    link(rel="stylesheet", href="https://fonts.googleapis.com/icon?family=Material+Icons")
    link(rel="stylesheet" href="https://file.myfontastic.com/Je2qVQFHWdpjAg3hYktk9P/icons.css")
    meta(name="viewport", content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0")
  body
    include blocks/header

    .u-gray.u-padding__bottom--small
      section.u-padding__top--small
        -
          var categorya = {
            "subtitle":"MI CUENTA"
          };
        +sectionSeparator(categorya).o-sectionSeparator--gray
      .container.u-margin__bottom--large
        .row
          .col.s12.l3
            .o-aside__atribute
              ul.o-collapsible.collapsible(data-collapsible="expandable")
                li.o-collapsible__item
                  .o-collapsible__header.o-collapsible__header--gray.o-collapsible__header--bgray.collapsible-header.active.u-padding__lr--none
                    |Hola, Mariella
                  .o-collapsible__body.collapsible-body
                    ul
                      li.c-listCategory__item
                        a(href="#!") Carrito
                      li.c-listCategory__item
                        a(href="#!") Favoritos
                      li.c-listCategory__item
                        a(href="#!") Historial de órdenes
                      li.c-listCategory__item
                        a(href="#!") Perfil
                      li.c-listCategory__item
                        a(href="#!") Cerrar sesión
          .col.s12.l9
            .row  
              .col.s12
                +sectionAcordion('DIRECCIÓN POR DEFECTO', '')
                  .row
                    each val in [1,2,3]
                      .col.s12.m6.l4
                        .card
                          .card-content
                            p Nombres y Apellidos
                            p Direccion, Calle especifica registrada por el usuario
                            p Ciudad, Código ZIP
                            p Pais
                            p Telefono: 000-000-000
                            p United States
                            p Phone: 987 645 321
                            .o-card__buttons
                              a(href="#!") Edit
                              a(href="#!") Delete
              .col.s12
                +sectionAcordion('OTRAS DIRECCIONES', '')
                  .row
                    .col.s12.m6.l4
                      .card
                        .card-content
                          p Nombres y Apellidos
                          p Direccion, Calle especifica registrada por el usuario
                          p Ciudad, Código ZIP
                          p Pais
                          p Telefono: 000-000-000
                          p United States
                          p Phone: 987 645 321
                          .o-card__buttons
                            a(href="#!") Edit
                            a(href="#!") Delete
                

      +sectionSeparator(section).o-sectionSeparator--bgray
        .row
          .col.s12
            section.novedades.c-productNews__container
              each val in [1,2,3,4,5,6,7,8,9]
                div
                  .c-productNews
                    .c-productNews__image
                      img(src="source/img/products/" + val + ".jpg", alt="")
                      a(href="#!") comprar
                        i.material-icons local_mall
                        | ahora
                    .c-productNews__info.c-productNews__info--nobottom
                      .c-productNews__name
                        h6 "La reina"
                        p PINTURA
                      .c-productNews__price
                        span 240

    include blocks/footer
    

    script(src="https://code.jquery.com/jquery-3.2.1.min.js")
    script(src="source/transpiled_js/materialize.min.js")
    script(src="source/transpiled_js/jquery.pinto.min.js")
    script(src="source/transpiled_js/slick.js")
    script(src="source/transpiled_js/script.js")